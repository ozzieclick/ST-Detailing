---
import Layout from "../../layouts/Layout.astro";
import Navbar from "../../components/navbar.astro";
import { t, changeLanguage } from "i18next";

changeLanguage("es");

const Title = t("layout.meta.title") || "";
const Description = t("layout.meta.description") || "";
const urlsite = Astro.url.toString();
---

<Layout  url={urlsite}  imagen="" Title={Title} Description={Description}>
<Navbar />
</Layout>


