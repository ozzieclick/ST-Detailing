---
import Layout from "../layouts/Layout.astro";
import Hero from "../components/Hero.astro";
import { t, changeLanguage } from "i18next";

changeLanguage("en");

const titulo = t("layout.meta.title") || "";
const Descripcion = t("layout.meta.description") || "";
const urlsite = Astro.url.toString();
---

<Layout url={urlsite} imagen="" Title={titulo} Description={Descripcion} >

 <Hero />   
</Layout>

